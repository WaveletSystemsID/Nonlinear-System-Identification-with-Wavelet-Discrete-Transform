function [ NMSE ] = NMSE( dn , en, n_points )
% plot erle and return fig object 
if nargin == 2
    n_points = 100; 
end


lendn = size(dn,1); 

indx = ceil(linspace(1, lendn, n_points)); 
    
NMSE = zeros(n_points,1);
for i = 1:n_points
    
    NSME(i) = ((en(1:indx(i))*en(1:indx(i))')/dn(1:indx(i))*dn(1:indx(i))'); 

end


end

